{
  "name": "trusteeshipTeacher",
  "app_id": "",
  "type": "ionic1",
  "gulpStartupTasks": [
    "process:sass",
    "process:typescript",
    "process:javascript",
    "watch"
  ],
  "watchPatterns": [
    "www/**/*",
    "!www/lib/**/*"
  ],
  "watch": {
    "sass": [
      "scss/**/*.scss"
    ],
    "html": [
      "www/**/*.html"
    ],
    "livereload": [
      "www/**/*.html",
      "www/**/*.js",
      "www/**/*.css"
    ]
  }
}
